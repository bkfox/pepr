{
    "plugins": [
        "plugins/markdown",
        "plugins/summarize",
        "node_modules/jsdoc-vuejs",
        "node_modules/jsdoc-export-default-interop/dist/index"
    ],
    "recurseDepth": 10,
    "source": {
        "include": ["assets/js", "assets/vue"],
        "includePattern": ".+\\.(js|vue)$",
        "excludePattern": "(^|\\/|\\\\)_|(\\.tests\\.js$)"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "useLongnameInNav": false
        }
    },
    "opts": {
        "encoding": "utf8"
    }
}

